{
  "name": "OpenRouter Configurable",
  "hidden": false,
  "user_variables": [ "MODEL", "API_KEY" ],
  "endpoint": "https://openrouter.ai/api/v1/chat/completions",
  "request_type": "POST",
  "request_headers": {
    "Authorization": "Bearer {{API_KEY}}"
  },
  "request_body": {
    "model": "{{MODEL}}",
    "messages": [
      {
        "role": "user",
        "content": "{{PROMPT}}"
      }
    ]
  },
  "response_jsonpath": "$.choices[*].message.content"
}
